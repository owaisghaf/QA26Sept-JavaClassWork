package com.example.mybeans;

public class Profile {
	
	String forename;
	String surname;
	double dateofbirth;
	String gender;
	String ethnicity;
	public Profile(String forename, String surname, double dateofbirth, String gender, String ethnicity) {
		super();
		this.forename = forename;
		this.surname = surname;
		this.dateofbirth = dateofbirth;
		this.gender = gender;
		this.ethnicity = ethnicity;
	}
	@Override
	public String toString() {
		return "Profile [forename=" + forename + ", surname=" + surname + ", dateofbirth=" + dateofbirth + ", gender="
				+ gender + ", ethnicity=" + ethnicity + "]";
	}
	
	


}
