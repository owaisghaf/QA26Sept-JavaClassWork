package streams;

public class MethodChaining {
	int x=10;
	
	public MethodChaining getObject() { 
		return this;
	}
	
	public MethodChaining AddtoX(int y) { 
		this.x=this.x+y;
		return this;
	}
	
	public MethodChaining msg(int y) { 
		return this;
	}

	public MethodChaining show() { 
		System.out.println(this.x);
		return this;
		}
}
